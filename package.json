    "start": "node index.ts",
    "local": "cross-env NODE_ENV=local nodemon --exec node $NODE_DEBUG_OPTION -r ts-node/register index.ts",
    "debug": "cross-env NODE_ENV=local nodemon --exec node --inspect -r ts-node/register index.ts",
    "debug:brk": "cross-env NODE_ENV=local nodemon --exec node --inspect-brk -r ts-node/register index.ts",
    "dev": "cross-env NODE_ENV=dev nodemon index.ts",
    "start:redis": "pm2 start redis-server --name redis-server",
    "clean": "pm2 delete all",
    "prd": "pm2 start index.ts",
